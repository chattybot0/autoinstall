<?php  function console_log($text = "No input specified, This means this must be a Crow.PHP FATAL error."){echo "<script>console.log('" . $text . "');</script>";} function includer($pack){echo $pack["require"];foreach ($pack["require"] as $name => $pack) {$composer = json_decode(file_get_contents("https://raw.githubusercontent.com/" . str_replace("-", "", $name) . "/master/composer.json"));if (count($composer["require"]) == 0) {echo "Load:" . $pack["name"];return true;} else {includer($composer);}}} function loadpkgs(){$read= fopen("composer.json", "r");$packs = json_decode(fread($read, filesize("composer.json")), true);}if (file_exists("composer.json")) {loadpkgs();} else {console_log("Crow.PHP Load error: No composer.json file, Making one...");$composerjson = fopen("composer.json","w");	fwrite($composerjson,'{ "require": { "mforoud86/crowdotphp": "^1.0" } }');	fclose($composerjson);}?>